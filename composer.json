{
    "name": "cart53/ci",
    "description": "CodeIgniter application installer.",
    "keywords": [
        "codeigniter"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Prashant Rijal",
            "email": "prashant@rijal.me"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "symfony/console": "^6.0",
        "symfony/process": "^6.0"
    },
    "bin": [
        "bin/ci"
    ],
    "autoload": {
        "psr-4": {
            "CodeIgniter\\Installer\\Console\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
